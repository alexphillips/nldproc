{
    "pipe" :
    {
        "processors" : [
            {
                "name"      : "lpf",
                    "type"      : "lpf",
                    "params"    :  []
            },
            {
                "name"      : "peakfollower",
                "type"      : "peakfollower",
                "params"    : []
            },
            {
                "name"      : "waveshaper",
                "type"      : "waveshaper",
                "params"    : []
            }
            ],
            "buffers" : [
            {
                "name"      : "lpf",
                "aliases"   : [
                    "lpf*peakfollower",
                "lpf*peakfollower (distorted)"
                ]
            },
            {
                "name"      : "peakfollower"
            }
        ],

        "operations" : [
            {
                "process_with"  : "lpf",
                "buffers" : {
                    "from"          : "input",
                    "to"            : "lpf"
                }
            },
            {
                "process_with"  : "peakfollower",
                "buffers" : {
                    "from"          : "input",
                    "to"            : "peakfollower"
                }
            },
            {
                "process_with"  : "multiply",
                "buffers" : {
                    "left"          : "lpf",
                    "right"         : "peakfollower",
                    "to"            : "lpf*peakfollower"
                }
            },
            {
                "process_with"  : "waveshaper",
                "buffers" : {
                    "from"          : "lpf*peakfollower",
                    "to"            : "lpf*peakfollower (distorted)"
                }
            },
            {

                "process_with"  : "clone",
                "buffers" : {
                    "from"          : "lpf*peakfollower (distorted)",
                    "to"            : "output"
                }
            }
        ]
    }
